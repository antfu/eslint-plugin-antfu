{
  "name": "eslint-plugin-antfu",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@8.11.0",
  "description": "Anthony extended ESLint rules",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-antfu#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-antfu.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-antfu/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.2.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.6",
    "@types/eslint": "^8.44.8",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.10.3",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/rule-tester": "^6.13.1",
    "@typescript-eslint/typescript-estree": "^6.13.1",
    "@typescript-eslint/utils": "^6.13.1",
    "ajv": "^6.10.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "eslint-define-config": "^2.0.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "lodash.merge": "4.6.2",
    "pnpm": "^8.11.0",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vite": "^5.0.5",
    "vitest": "^1.0.0-beta.6"
  },
  "resolutions": {
    "eslint-plugin-antfu": "workspace:*"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
